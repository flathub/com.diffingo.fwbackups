{
  "app-id": "com.diffingo.fwbackups",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "fwbackups",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland",
    "--filesystem=host",
    "--share=network",
    "--socket=ssh-auth",
    "--talk-name=org.freedesktop.Flatpak"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    "python3-modules.json",
    {
      "name": "fwbackups",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/stewartadam/fwbackups.git",
          "commit": "1cc0a3cfa6eec8e48d18bf811f7876ba189d7fa9"
        }
      ]
    }
  ]
}
